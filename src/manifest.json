{
    "manifest_version": 3,
    "name": "Better Wiki",
    "version":"1.1",
    "icons": {
        "24": "icons/icon-24.png",
        "48": "icons/icon-48.png",
        "64": "icons/icon-64.png",
        "128": "icons/icon-128.png"
    },

    "description": "Shows table of content at a fixed position for better navigation through wikipedia pages",

    "content_scripts": [
        {
            "matches":["*://*.wikipedia.org/wiki/*"],
            "js": ["content.js"]
        }
    ]
}